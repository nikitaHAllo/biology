PORT=3000
NODE_ENV=development

# Токен Telegram-бота
BOT_TOKEN=your_bot_token_here

# URL фронтенда (Mini App) — должен быть HTTPS для web_app кнопки
FRONTEND_URL=https://your-miniapp-url.example
# Строка подключения к PostgreSQL
# Формат: postgres://<пользователь>:<пароль>@<хост>:<порт>/<база_данных>
DATABASE_URL=postgres://postgres:postgres@localhost:5432/biology
# ID чата/канала для уведомлений админа (число)
ADMIN_CHAT_ID=123456789
# Стоимость отправки задания 2-й части (в репкоинах)
ASSIGNMENT_PRICE=10
